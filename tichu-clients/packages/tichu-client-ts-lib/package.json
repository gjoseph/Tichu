{
  "name": "tichu-client-ts-lib",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "build": "npm run clean:some && tsc --build && ava",
    "clean:some": "rm -rf ./lib ./docs",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish",
    "docs": "typedoc --out docs ./src",
    "test": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "engines": {
    "node": "16.13.1",
    "npm": "8.3.0"
  },
  "devDependencies": {
    "@ava/typescript": "2.0.0",
    "@liftr/tscov": "2.0.0",
    "@types/node": "16.11.18",
    "@types/ws": "7.4.7",
    "ava": "4.0.0",
    "typedoc": "0.22.10",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@types/fs-extra": "9.0.13",
    "fs-extra": "10.0.0",
    "nanoid": "3.1.30",
    "ts-enum-util": "4.0.2",
    "tslib": "2.3.1"
  },
  "optionalDependencies": {
    "ws": "7.5.6"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      },
      "compile": false
    }
  }
}
