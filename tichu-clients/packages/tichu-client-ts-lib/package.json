{
  "name": "tichu-client-ts-lib",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "build": "npm run clean:some && tsc --build && ava",
    "clean:some": "rm -rf ./lib ./docs",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish",
    "docs": "typedoc --out docs ./src",
    "test": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "engines": {
    "node": "14.18.1",
    "npm": "7.24.1"
  },
  "devDependencies": {
    "@ava/typescript": "1.1.1",
    "@liftr/tscov": "1.4.8",
    "@types/node": "14.17.3",
    "@types/ws": "7.4.0",
    "ava": "3.15.0",
    "typedoc": "0.17.8",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "@types/fs-extra": "9.0.0",
    "fs-extra": "9.0.0",
    "nanoid": "3.1.22",
    "ts-enum-util": "4.0.2",
    "tslib": "1.11.1"
  },
  "optionalDependencies": {
    "ws": "7.4.4"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      }
    }
  }
}
