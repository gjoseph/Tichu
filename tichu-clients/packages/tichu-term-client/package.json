{
  "name": "tichu-term-client",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "bin": {
    "tichu": "./bin/index.js"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "build": "npm run clean:some && tsc --build && ava",
    "clean:some": "rm -rf ./lib ./docs",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish",
    "docs": "typedoc --out docs ./src",
    "test": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "engineStrict": true,
  "engines": {
    "node": ">=13.7.0"
  },
  "devDependencies": {
    "@ava/typescript": "1.1.1",
    "@liftr/tscov": "1.4.7",
    "@types/inquirer": "6.5.0",
    "@types/node": "13.13.12",
    "@types/ws": "7.2.6",
    "ava": "3.9.0",
    "typedoc": "0.17.8",
    "typescript": "3.9.6"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.0",
    "commander": "^5.0.0",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "kleur": "^4.0.0",
    "tichu-client-ts-lib": "1.0.0",
    "ts-enum-util": "^4.0.1",
    "tslib": "^2.0.0",
    "ws": "^7.2.3"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      }
    }
  }
}
