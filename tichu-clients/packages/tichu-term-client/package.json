{
  "name": "tichu-term-client",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "bin": {
    "tichu": "bin/index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "build": "npm run clean:some && tsc --build && ava",
    "clean:some": "rm -rf ./lib ./docs",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish",
    "docs": "typedoc --out docs ./src",
    "test": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "engines": {
    "node": "22.11.0",
    "npm": "8.14.0"
  },
  "devDependencies": {
    "@ava/typescript": "3.0.1",
    "@liftr/tscov": "2.0.0",
    "@types/inquirer": "9.0.3",
    "@types/node": "22.9.0",
    "@types/ws": "8.5.4",
    "ava": "4.3.3",
    "typedoc": "0.23.26",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@types/fs-extra": "9.0.13",
    "commander": "10.0.0",
    "fs-extra": "10.1.0",
    "inquirer": "9.1.4",
    "kleur": "4.1.5",
    "tichu-client-ts-lib": "1.0.0",
    "ts-enum-util": "4.0.2",
    "tslib": "2.4.1",
    "ws": "8.8.1"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      },
      "compile": false
    }
  }
}
