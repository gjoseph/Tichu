{
  "name": "tichu-term-client",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "bin": {
    "tichu": "bin/index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "build": "npm run clean:some && tsc --build && ava",
    "clean:some": "rm -rf ./lib ./docs",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish",
    "docs": "typedoc --out docs ./src",
    "test": "npm run build",
    "lint": "eslint src/"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "engines": {
    "node": "22.16.0",
    "npm": "11.4.2"
  },
  "devDependencies": {
    "@ava/typescript": "6.0.0",
    "@types/node": "22.15.35",
    "@types/ws": "8.18.1",
    "ava": "6.4.1",
    "typedoc": "0.28.14",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@types/fs-extra": "11.0.4",
    "commander": "14.0.1",
    "fs-extra": "11.3.2",
    "inquirer": "12.9.6",
    "kleur": "4.1.5",
    "tichu-client-ts-lib": "1.0.0",
    "ts-enum-util": "4.1.0",
    "tslib": "2.8.1",
    "ws": "8.18.3"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      },
      "compile": false
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ]
  }
}
